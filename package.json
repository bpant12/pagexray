{
  "name": "pagexray",
  "version": "0.14.3",
  "description": "Xray your HAR file and know all about the page",
  "keywords": [
    "performance",
    "web",
    "har",
    "webperf",
    "perfmatters"
  ],
  "homepage": "http://www.sitespeed.io",
  "license": "MIT",
  "author": {
    "name": "Peter Hedenskog",
    "url": "http://www.peterhedenskog.com"
  },
  "contributors": [
    {
      "name": "Tobias Lidskog"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:sitespeedio/pagexray.git"
  },
  "bugs": {
    "url": "https://github.com/sitespeedio/pagexray/issues"
  },
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "lint": "npm run eslint",
    "eslint": "eslint .",
    "test": "mocha",
    "jsdoc": "jsdoc lib/* -d dist/doc",
    "travis": "npm run lint && npm run test",
    "browserify": "mkdirp dist && browserify lib/index.js -o dist/pagexray.js --standalone PageXray -p [ browserify-banner --template 'PageXray v<%= pkg.version %> - Xray your HAR file and know all about the page - https://github.com/sitespeedio/pagexray' ]",
    "uglify": "uglifyjs dist/pagexray.js --compress --mangle --comments '/pagexray/' > dist/pagexray.min.js",
    "prepublishOnly": "npm run browserify && npm run uglify"
  },
  "engines": {
    "node": ">=6.10.3"
  },
  "devDependencies": {
    "bluebird": "^3.3.5",
    "browserify": "14.4.0",
    "browserify-banner": "1.0.4",
    "chai": "^4.0.1",
    "eslint": "^3.5.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.3.0",
    "jsdoc": "^3.4.0",
    "lodash.foreach": "^4.2.0",
    "lodash.pluck": "^3.1.2",
    "mocha": "^3.0.2",
    "uglify-es": "^3.0.24"
  },
  "main": "./lib/index.js",
  "bin": "./bin/index.js",
  "dependencies": {
    "minimist": "1.2.0"
  }
}
